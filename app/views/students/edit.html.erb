<div  class="row">
  <div  class="col-lg-8 portlets">
    <div class="text-right" style="margin-bottom: 10px">
      <%= link_to students_path, :class=>'btn btn-info',:data => { :no_turbolink => true } do %>
          <i class='fa fa-align-left fa-reply-all'></i> All Students
      <% end %>
    </div>

    <div  class="widget">
      <div class="widget-header">
        <h2><i class="fa fa-user"></i> <strong>Student Edit Form</strong> </h2>

      </div>
      <div class="widget-content">
        <div class="row stacked">
          <div class="col-sm-12 portlets">

            <div class="widget">

              <div class="widget-content padding">
                <div id="basic-form">
                  <%= render 'students/form_student' %>
                </div>
              </div>
            </div>

          </div>


        </div>

      </div>
    </div>
  </div>
  <div class="col-lg-4 portlets">


  </div>
</div>
